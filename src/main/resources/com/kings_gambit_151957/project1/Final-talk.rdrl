package com.kings_gambit_151957.project1;

import java.lang.Number;

rule "Final-talk"
	dialect "mvel"
	ruleflow-group "final-talk"
	when
		Equipment : Equipment( this != null )
	then
		Equipment.setExperience(Equipment.getExperience() + 400); Equipment.getItems().add("Death From Above crossbow");
end
